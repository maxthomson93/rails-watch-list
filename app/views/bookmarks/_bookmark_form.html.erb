  <%= simple_form_for [@list, @bookmark] do |f|%>
  <%= f.input :movie_id, collection: @movies, label_method: :title, value_method: :id, prompt: "Select a movie" %>
  <%= f.input :comment %>
  <%= f.submit 'Submit', class: 'btn btn-success' %>
  <%= link_to lists_path, class: 'btn btn-primary' do  %>
    <i class="fa-solid fa-house"></i>
  <% end %>
<% end %>
